{
  "kind": "collectionType",
  "collectionName": "product_categories",
  "info": {
    "singularName": "product-category",
    "pluralName": "product-categories",
    "displayName": "ProductCategory",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "category_name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "products": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::product.product",
      "mappedBy": "product_categories"
    },
    "category_name_en": {
      "type": "string",
      "unique": true,
      "required": true
    },
    "category_icon": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "feature_stack": {
      "type": "integer",
      "required": true,
      "default": -1
    }
  }
}
